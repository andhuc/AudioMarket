@{
    Audio mostLikedAudio = ViewBag.mostLikedAudio;
}

<div class="text-center">
    
    <div class="container">
        <h1>Big Input Form @mostLikedAudio.title</h1>
        <form>
            <div class="form-group">
                <label for="inputField">Input Field:</label>
                <input type="text" class="form-control" id="inputField" placeholder="Enter something...">
            </div>
            <div class="form-group">
                <label for="textareaField">Textarea:</label>
                <textarea class="form-control" id="textareaField" rows="5" placeholder="Enter a larger text..."></textarea>
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>
    </div>

</div>
